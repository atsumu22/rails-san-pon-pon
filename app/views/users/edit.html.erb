<div class="container my-5">
  <% if current_user.status == "chairperon" %>
    <%= link_to dashboard_path do %>
      <p class="mb-3"><i class="fa fa-solid fa fa-chevron-left me-3"></i>Go back to dashboard</p>
    <% end %>
  <% else %>
    <%= link_to dashboard_path do %>
      <p class="mb-3"><i class="fa fa-solid fa fa-chevron-left me-3"></i>Go back to profile</p>
    <% end %>
  <% end %>

  <div>
    <h1 class="mb-3">Edit your profile</h1>
    <%= render 'users/form' %>
  </div>
</div>
